1. Print pattern which should print below
******
******
******
sol:
#include <bits/stdc++.h>
using namespace std;

int main(){
int n,m;
cin>>n;
cin>>m;

for(int i=1;i<=n;i++){
   for(int j=1;j<=6;j++){
   cout<<"*";
   }
   cout<<endl;
}
return 0;
}

2. To print below pattern
*****
*   *
*****
#include <bits/stdc++.h>
using namespace std;
int main() {
    // Write C++ code here
    int n,m;
    cin>>n>>m;
    for(int i=1;i<=n;i++){
        for (int j=1;j<=m;j++){
            if(i==1||j==1||i==n||j==m){
                cout<<"*";
            }
            else{
                cout<<" ";
            }
        }
        cout<<endl;
    }

    return 0;
}
3. code for print below pattern
*
**
***
****

#include <bits/stdc++.h>
using namespace std;
int main() {
    // Write C++ code here
   int n;
   cin>>n;
   for(int i=1;i<=n;i++){
       for(int j=1;j<=i;j++){
           cout<<"*";
       }
       cout<<endl;
   }
}
4. To print below pattern

   *
  ***
 *****
*******

#include <bits/stdc++.h>
using namespace std;
int main() {
    // Write C++ code here
   int n;
   cin>>n;
   for(int i=1;i<=n;i++){
       for(int j=1;j<=(n-i);j++){
           cout<<" ";
       }
       for(int j=1;j<=(2*i-1);j++){
           cout<<"*";
       }
       cout<<endl;
   }
}
5. Numerical ractangular pattern
123456
234561
345612
456123
561234
612345

#include <bits/stdc++.h>
using namespace std;
int main() {
    // Write C++ code here
    int n;
    cin>>n;
    for(int i=1;i<=n;i++){
        for(int j=i;j<=n;j++){
            cout<<j;
        }
        for(int j=1;j<=(i-1);j++){
            cout<<j;
        }
        cout<<endl;
    }
    return 0;
}
